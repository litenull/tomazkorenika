    link(rel='stylesheet', href='/css/bootstrap.css')
    script(src='http://code.jquery.com/jquery-2.0.2.min.js')
  body
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          //-img(src='/avatar.png',width='34px', style='float:left; margin-right:10px')
